{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug TSX",
			"type": "node",
			"request": "launch",
			"program": "${file}",
			"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/tsx",
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"skipFiles": [
				"<node_internals>/**",
				"${workspaceFolder}/node_modules/**"
			]
		}
	]
}
